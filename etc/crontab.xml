<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
	<group id="default">
		<job instance="OrviSoft\FeedCronScripts\Cron\GoogleFeed" method="execute" name="orvisoft_feedcronscripts_googlefeed">
			<schedule>0 */3 * * *</schedule>
		</job>
		<job instance="OrviSoft\FeedCronScripts\Cron\PixedFeed" method="execute" name="orvisoft_feedcronscripts_pixedfeed">
			<schedule>0 */3 * * *</schedule>
		</job>
		<job instance="OrviSoft\FeedCronScripts\Cron\PriceSearcher" method="execute" name="orvisoft_feedcronscripts_pricesearcher">
			<schedule>0 */3 * * *</schedule>
		</job>
		<job instance="OrviSoft\FeedCronScripts\Cron\WebCollage" method="execute" name="orvisoft_feedcronscripts_webcollage">
			<schedule>0 */3 * * *</schedule>
		</job>
	</group>
</config>
